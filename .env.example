# Docker Compose Environment Configuration Example
# Copy this file to .env and update values for your environment
# This file is used by docker-compose.yml

# MySQL Configuration
MYSQL_ROOT_PASSWORD=your_mysql_root_password
MYSQL_DATABASE=chatapp

# Neo4j Configuration
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password

# Backend Configuration
DATABASE_URL=mysql+mysqlconnector://root:${MYSQL_ROOT_PASSWORD}@mysql:3306/${MYSQL_DATABASE}
NEO4J_URI=bolt://neo4j:7687
SECRET_KEY=your-secret-key-change-this-in-production
ALGORITHM=HS256

# Frontend Configuration
VITE_API_URL=http://localhost:8000

# Optional: Service Ports (adjust if needed)
MYSQL_PORT=3306
NEO4J_BOLT_PORT=7687
NEO4J_HTTP_PORT=7474
BACKEND_PORT=8000
FRONTEND_PORT=3000
